//https://cses.fi/problemset/task/1083/
#include<iostream>
using namespace std;
using ll=long long;
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    ll n;
    cin>>n;
    ll ans=(n*(n+1))/2,sum=0;
    n--;
    while(n--){
    	int a;
    	cin>>a;
    	sum+=a;
    }
    cout<<ans-sum<<endl;
}
