//https://cses.fi/problemset/task/1634/
#include<iostream>
#include<iomanip>
#include<climits>
#include<cmath>
#include<vector>
#include<map>
#include<queue>
#include<algorithm>
#include<cstring>
using namespace std;
using ll=long long;
int mod=1e9+7;
ll dp[1000001];
ll funct(int arr[],int k,int n)
{
    if (k==0) return 0;
    if (dp[k]!=-1) return dp[k];
    ll ans=INT_MAX;
    for (int i=0;i<n;i++){
        if (arr[i]<=k) ans=min(ans,1+funct(arr,k-arr[i],n));
    }
    return dp[k]=ans;
}
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    memset(dp,-1,sizeof dp);
    int n,k;
    cin>>n>>k;
    int arr[n];
    for (int i=0;i<n;i++) cin>>arr[i];
    int ans=funct(arr,k,n);
    if (ans==INT_MAX) cout<<"-1\n";
    else cout<<ans<<endl;
}
